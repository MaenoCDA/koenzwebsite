{
	"kind": "collectionType",
	"collectionName": "posts",
	"info": {
		"singularName": "post",
		"pluralName": "posts",
		"displayName": "Nieuwsbericht",
		"description": ""
	},
	"options": {
		"draftAndPublish": true
	},
	"pluginOptions": {
		"i18n": {
			"localized": true
		}
	},
	"attributes": {
		"title": {
			"type": "string",
			"required": true,
			"pluginOptions": {
				"i18n": {
					"localized": true
				}
			}
		},
		"seo": {
			"type": "component",
			"repeatable": false,
			"component": "shared.seo",
			"required": false,
			"pluginOptions": {
				"i18n": {
					"localized": true
				}
			}
		},
		"hero": {
			"type": "dynamiczone",
			"pluginOptions": {
				"i18n": {
					"localized": true
				}
			},
			"repeatable": false,
			"components": ["hero.text"],
			"required": true,
			"min": 1,
			"max": 1
		},
		"resources": {
			"type": "component",
			"component": "shared.resources",
			"repeatable": false,
			"required": false
		},
		"flexContent": {
			"pluginOptions": {
				"i18n": {
					"localized": true
				}
			},
			"type": "dynamiczone",
			"components": [
				"content.accordion",
				"content.image",
				"content.image-text",
				"content.text",
				"content.video",
				"content.quote",
				"content.form",
				"content.cta",
				"teaser.manual-teaser",
				"teaser.dynamic-teaser",
				"teaser.latest-teaser",
				"content.brands"
			],
			"required": false
		},
		"slug": {
			"pluginOptions": {
				"i18n": {
					"localized": true
				}
			},
			"type": "uid",
			"targetField": "title",
			"required": true
		},
		"parentUid": {
			"pluginOptions": {
				"i18n": {
					"localized": true
				}
			},
			"type": "enumeration",
			"enum": ["api::overview.overview"],
			"default": "api::overview.overview",
			"required": true
		},
		"rank": {
			"pluginOptions": {
				"i18n": {
					"localized": true
				}
			},
			"type": "integer"
		},
		"tags": {
			"type": "relation",
			"relation": "manyToMany",
			"target": "api::tag.tag",
			"mappedBy": "api_post_post"
		},
		"image": {
			"allowedTypes": ["images"],
			"type": "media",
			"multiple": false,
			"pluginOptions": {
				"i18n": {
					"localized": true
				}
			},
			"required": true
		}
	}
}
