{
  "version": 2,
  "builds": [
    {
      "src": "apps/cms/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/cms/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "apps/cms/index.js": {
      "maxDuration": 30
    }
  }
}