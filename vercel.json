{
  "version": 2,
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/web/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_CMS_URL": "@cms_url",
    "NEXT_PUBLIC_IMAGE_BASE_URL": "@cms_url"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_CMS_URL": "@cms_url",
      "NEXT_PUBLIC_IMAGE_BASE_URL": "@cms_url"
    }
  }
}