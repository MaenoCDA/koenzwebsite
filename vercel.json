{
  "version": 2,
  "buildCommand": "corepack enable && yarn --cwd apps/web build",
  "outputDirectory": "apps/web/.next",
  "installCommand": "corepack enable && yarn install --check-cache",
  "framework": "nextjs",
  "env": {
    "NODE_VERSION": "20.17.0"
  },
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next",
      "config": {
        "maxDuration": 60
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/web/$1"
    }
  ]
}